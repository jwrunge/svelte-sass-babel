{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "rollup-dev": "rollup -c -w",
    "rollup-prod": "rollup -c",
    "sass-dev": "sass --watch src/style/core.scss public/build/bundle.css",
    "sass-prod": "sass --style=compressed src/style/core.scss public/build/bundle.css",
    "start": "sirv public --no-clear",
    "prod": "run-s sass-prod rollup-prod",
    "dev": "run-p sass-dev rollup-dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-external-helpers": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "babel-preset-minify": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.43.2",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.9.8"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
